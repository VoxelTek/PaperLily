label START
    if not var ch1.temp_ending_1_3 end
    action turn up
    camera move (0,-50) time 4
    pause 2
    camera unlock
    action walk up 32 speed veryslow continue
    pause 0.25
    cg show black time 0

    pause 2
    "Dead End 1-3 - A Walk in the Dark" background none
    system achievement CH1_BAD_END
    set persistent.ending.1.3 = true
    system title-screen
